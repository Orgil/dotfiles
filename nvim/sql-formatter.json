{
  "language": "postgresql",
  "tabWidth": 2,
  "keywordCase": "upper",
  "linesBetweenQueries": 2,
  "logicalOperatorNewline": "after",
  "paramTypes": {
    "named": [
      ":",
      "@"
    ],
    "custom": [
      {
        "regex": "sqlc\\.narg\\('[a-zA-Z0-9_]+'\\)"
      },
      {
        "regex": "sqlc\\.arg\\('[a-zA-Z0-9_]+'\\)"
      },
      {
        "regex": "uuid_generate_v4\\(\\)"
      }
    ]
  }
}
